            type: 'GET',
            dataType: 'json'
        }).done(function (data) {
            $("#KapiListesi").html('');
            $.each(data, function (index, row) {
                $("#KapiListesi").append('<div class="custom-control custom-checkbox"><input type = "checkbox" value = "' + row.Value + '" name = "Kapi" class= "custom-control-input" id = "' + ('Kapi' + row.Value) + '" /><label class="custom-control-label" for="' + ('Kapi' + row.Value) + '">' + row.Text + '</label></div >');
            });
        });

        $("#Panel").chosen().change(function () {
            var panelNo = $(this).val();
            $.ajax({
                url: '/ReportPersonel/PanelKapiListesi?PanelNo=' + panelNo,
                type: 'GET',
                dataType: 'json'
            }).done(function (data) {
                $("#KapiListesi").html('');
                $.each(data, function (index, row) {
                    $("#KapiListesi").append('<div class="custom-control custom-checkbox"><input type = "checkbox" value = "' + row.Value + '" name = "Kapi" class= "custom-control-input" id = "' + ('Kapi' + row.Value) + '" /><label class="custom-control-label" for="' + ('Kapi' + row.Value) + '">' + row.Text + '</label></div >');
                });
            });

        })




    });
</script>


<script>

    $(document).ready(function () {
        $('.chosen-select').chosen();
        $('.chosen-select-deselect').chosen({ allow_single_deselect: true });
        $("#Departman").chosen().change(function () {
            var departmanNo = $(this).val();
            if (departmanNo == 0 || departmanNo == null) {
                $("#AltDepartman").empty();
                $("#Bolum").empty();
                $("#Birim_No").empty();
                $("#AltDepartman").append('<option > Alt Departman Seçiniz... </option>');
                $("#Bolum").append('<option > Bölüm Seçiniz... </option>');
                $("#Birim_No").append('<option > Birim Seçiniz... </option>');
            } else {
                $.ajax({
                    url: '/ReportPersonel/AltDepartmanListesi?Departman=' + departmanNo,
                    type: 'GET',
                    dataType: 'json'
                }).done(function (data) {
                    $("#AltDepartman").empty();
                    $("#AltDepartman").append('<option > Alt Departman Seçiniz... </option>');
                    $.each(data, function (index, row) {
                        $("#AltDepartman").append('<option value=' + row.Value + '>' + row.Text + '</option>').trigger("chosen:updated");
                    });
                    var altdepartmanNo = $("#AltDepartman").val();
                    $.ajax({
                        url: '/ReportPersonel/BolumListesi?AltDepartman=' + altdepartmanNo,
                        type: 'GET',
                        dataType: 'json'
                    }).done(function (data) {
                        $("#Bolum").empty();
                        $.each(data, function (index, row) {
                            $("#Bolum").append('<option value=' + row.Value + '>' + row.Text + '</option>').trigger("chosen:updated");
                        });
                        var bolumNo = $("#Bolum").val();
                        $.ajax({
                            url: '/ReportPersonel/BirimListesi?AltDepartman=' + altdepartmanNo + '&Bolum=' + bolumNo,
                            type: 'GET',
                            dataType: 'json'
                        }).done(function (data) {
                            $("#Birim_No").empty();
                            $.each(data, function (index, row) {
                                $("#Birim_No").append('<option value=' + row.Value + '>' + row.Text + '</option>').trigger("chosen:updated");
                            });
                        });
                    });

                });
            }
        });
        $("#AltDepartman").chosen().change(function () {
            var altdepartmanNo = $(this).val();
            $.ajax({
                url: '/ReportPersonel/BolumListesi?AltDepartman=' + altdepartmanNo,
                type: 'GET',
                dataType: 'json'
            }).done(function (data) {
                $("#Bolum").empty();
                $.each(data, function (index, row) {
                    $("#Bolum").append('<option value=' + row.Value + '>' + row.Text + '</option>').trigger("chosen:updated");
                });
                var bolumNo = $("#Bolum").val();
                $.ajax({
                    url: '/ReportPersonel/BirimListesi?AltDepartman=' + altdepartmanNo + '&Bolum=' + bolumNo,
                    type: 'GET',
                    dataType: 'json'
                }).done(function (data) {
                    $("#Birim_No").empty();
                    $.each(data, function (index, row) {
                        $("#Birim_No").append('<option value=' + row.Value + '>' + row.Text + '</option>').trigger("chosen:updated");
                    });
                });




            });
        })

        $("#Bolum").chosen().change(function () {
            var bolumNo = $(this).val();
            var altdepartmanNo = $("#AltDepartman").val();
            $.ajax({
                url: '/ReportPersonel/BirimListesi?AltDepartman=' + altdepartmanNo + '&Bolum=' + bolumNo,
                type: 'GET',
                dataType: 'json'
            }).done(function (data) {
                $("#Birim_No").empty();
                $.each(data, function (index, row) {
                    $("#Birim_No").append('<option value=' + row.Value + '>' + row.Text + '</option>').trigger("chosen:updated");
                });
            });
        })







        $("tfoot a").addClass('btn btn-sm  btn-default'); // buton normal durum css
        // buton aktif durum css
        $("tfoot td")
            .contents()
            .filter(function () {
                if (this.nodeType === 3 && this.length > 1) {
                    return this.nodeType
                }
            })
            .wrap('<span class=" btn btn-primary" />');


        $("#myTable tbody tr").click(function () {
            var kayitNo = $(this).children("td").data("kayitno");
            var name = $(this).children("td").data("visitor");
            $("#Ziyaretci").attr('value', name);
            $("#UserKayitNo").attr('value', kayitNo);
            var selected = $(this).hasClass("table-success");
            $("#myTable tbody tr").removeClass("table-success");
            if (!selected)
                $(this).addClass("table-success");
        });
    });
</script>


<script>
    var today = new Date();
    $('#Tarih1').val(today.getFullYear() + '-' + ('0' + (today.getMonth() + 1)).slice(-2) + '-' + ('0' + today.getDate()).slice(-2));
    $('#Tarih2').val(today.getFullYear() + '-' + ('0' + (today.getMonth() + 1)).slice(-2) + '-' + ('0' + today.getDate()).slice(-2));


    $("#ikiTarih").change(function () {
        if ($(this).prop("checked") == true) {
            $("#Tarih2").attr("disabled", false);
        } else {
            $("#Tarih2").attr("disabled", true);
        }

    });
    $("#TumKapi").change(function () {
        if ($(this).prop("checked") == true) {
            $("#Kapi1").attr("disabled", true);
            $("#Kapi1").prop("checked", false);
            $("#Kapi2").attr("disabled", true);
            $("#Kapi2").prop("checked", false);
            $("#Kapi3").attr("disabled", true);
            $("#Kapi3").prop("checked", false);
            $("#Kapi4").attr("disabled", true);
            $("#Kapi4").prop("checked", false);
            $("#Kapi5").attr("disabled", true);
            $("#Kapi5").prop("checked", false);
            $("#Kapi6").attr("disabled", true);
            $("#Kapi6").prop("checked", false);
            $("#Kapi7").attr("disabled", true);
            $("#Kapi7").prop("checked", false);
            $("#Kapi8").attr("disabled", true);
            $("#Kapi8").prop("checked", false);
        } else {
            $("#Kapi1").attr("disabled", false);
            $("#Kapi2").attr("disabled", false);
            $("#Kapi3").attr("disabled", false);
            $("#Kapi4").attr("disabled", false);
            $("#Kapi5").attr("disabled", false);
            $("#Kapi6").attr("disabled", false);
            $("#Kapi7").attr("disabled", false);
            $("#Kapi8").attr("disabled", false);
        }

    });
    $("#TümKullanici").change(function () {
        if ($(this).prop("checked") == true) {
            $("#Ziyaretci").attr("disabled", true);
            $("#UserKayitNo").attr("value", null);
            $("#Ziyaretci").attr("value", null);
        } else {
            $("#Ziyaretci").attr("disabled", false);
        }
    });
    $("#temizle").click(function () {
        $("#UserKayitNo").attr("value", null);
        $("#Ziyaretci").attr("value", null);
        $("#alert").text("");
    });
    $("#TümTarih").change(function () {
        if ($(this).prop("checked") == true) {
            $("#ikiTarih").attr("disabled", true);
            $("#ikiSaat").attr("disabled", true);
            $("#Tarih1").attr("disabled", true);
            $("#Tarih2").attr("disabled", true);
            $("#Saat1").attr("disabled", true);
            $("#Saat2").attr("disabled", true);
        } else {
            $("#ikiTarih").attr("disabled", false);
            $("#ikiSaat").attr("disabled", false);
            $("#Tarih1").attr("disabled", false);
        }

    });
    $("#ikiSaat").change(function () {
        if ($(this).prop("checked") == true) {
            $("#Saat1").attr("disabled", false);
            $("#Saat2").attr("disabled", false);
        } else {
            $("#Saat1").attr("disabled", true);
            $("#Saat2").attr("disabled", true);
        }

    });
    $("#Ziyaretci").focus(function () {
        $("#exampleModal").modal("show");
        $("#myTable tbody tr").click(function () {
            var kayitNo = $(this).children("td").data("kayitno");
            var name = $(this).children("td").data("visitor");
            $("#Ziyaretci").attr('value', name);
            $("#UserKayitNo").attr('value', kayitNo);
        });
    });


    $("#btnPrint").click(function () {
        var contents = $("#print_div").html();
        var frame1 = $('<iframe />');
        frame1[0].name = "frame1";
        frame1.css({ "position": "absolute" });
        $("body").append(frame1);
        var frameDoc = frame1[0].contentWindow ? frame1[0].contentWindow : frame1[0].contentDocument.document ? frame1[0].contentDocument.document : frame1[0].contentDocument;
        frameDoc.document.open();
        //Create a new HTML document.
        frameDoc.document.write('<html>');
        frameDoc.document.write('<head>');
        frameDoc.document.write('<link href="style.css" rel="stylesheet" type="text/css" media="print" />');
        frameDoc.document.write('<style>');
        frameDoc.document.write('table{ border-collapse: collapse; width: 100%;}')
        frameDoc.document.write('table th, td { text-align: center; padding: 8px;} body{padding:10px;}');
        frameDoc.document.write('tr:nth-child(even) {background-color: #f2f2f2;}');
        frameDoc.document.write('table tr > *:nth-child(1){ display: none; }');
        frameDoc.document.write('table tr > *:nth-child(8){ display: none; }');
        frameDoc.document.write('table tr > *:nth-child(9){ display: none; }');
        frameDoc.document.write('table tr > *:nth-child(15){ display: none; }');
        frameDoc.document.write('table tfoot > *:nth-child(2){ display: none; }');
        frameDoc.document.write('table, th, td {  border: 1px solid black; text-align:center; border-collapse: collapse;}');
        frameDoc.document.write('thead tr th a { color: black; text-decoration: none; }thead tr th a:hover { color: black; }thead tr th a:visited { color: black; }thead tr th a:active { color: black; }');
        frameDoc.document.write('</style>');
        frameDoc.document.write('</head>');
        frameDoc.document.write('<body>');
        frameDoc.document.write('<h5>Tarih (' + new Date().toLocaleDateString() + ')</h5><hr/>');
        frameDoc.document.write('<h5>Aktif Personel Listesi ' + '@TempData["DateAndTimeView"]' + '</h5><hr/>');
        frameDoc.document.write(contents);
        frameDoc.document.write('</body>');
        frameDoc.document.write('</html>');
        frameDoc.document.close();
        setTimeout(function () {

            window.frames["frame1"].focus();
            window.frames["frame1"].print();
            frame1.remove();
        }, 500);

    });
</script>
