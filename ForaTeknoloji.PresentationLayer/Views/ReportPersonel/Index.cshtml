@model ForaTeknoloji.PresentationLayer.Models.ReportPersonelViewModel
@using GridMvc.Html;
@using ForaTeknoloji.Entities.ComplexType
@{
    /**/

    ViewBag.Title = "Personel Raporları";
    Layout = "~/Views/Shared/_Layout.cshtml";
    //Layout = "~/Views/Shared/_LayoutNewTemplate.cshtml";
    WebGrid webGrid = new WebGrid(source: Model.ReportPersonel, rowsPerPage: 20, canPage: true, selectionFieldName: "selectedRow", ajaxUpdateContainerId: "gridContent");
    webGrid.Pager(WebGridPagerModes.NextPrevious);
    string img = "#";
    string canlimg = "#";
    if (webGrid.HasSelection)
    {
        var sonuc = (ForaTeknoloji.Entities.ComplexType.ReportPersonelList)webGrid.Rows[webGrid.SelectedIndex].Value;
        img = "/Images/" + sonuc.Resim;
        canlimg = "/Canli_Images/" + sonuc.Canli_Resim;

    }
}

<style>
    .table-pager > td {
        padding-top: 10px;
    }

        .table-pager > td > a {
            background-color: #f5f5f5;
            border: 1px solid #ccc;
            border-radius: 3px;
            padding: 3px 7px;
        }

            .table-pager > td > a:hover {
                background-color: #f0f0f0;
            }
</style>
<div class="card border-left-info h-100 mt-1 shadow-lg">
    <div class="card-header py-1">
        <h3 class="card-title"><i class="fas fa-bookmark"></i> Aktif Kullanıcı Raporları</h3>
    </div>
    <div class="card-body">
        @using (Html.BeginForm("Index", "ReportPersonel", FormMethod.Get))
        {
            <div class="row">
                <div class="col-md-2">
                    <div class="form-group">
                        <div class="custom-control custom-radio">
                            <input type="radio" value="3" class="custom-control-input" id="tumYon" checked name="Kapi_Yon">
                            <label class="custom-control-label" for="tumYon">Tüm Yönler</label>
                        </div>
                        <div class="custom-control custom-radio">
                            <input type="radio" value="0" class="custom-control-input" id="giris" name="Kapi_Yon">
                            <label class="custom-control-label" for="giris">Giriş</label>
                        </div>
                        <div class="custom-control custom-radio">
                            <input type="radio" value="1" class="custom-control-input" id="cikis" name="Kapi_Yon">
                            <label class="custom-control-label" for="cikis">Çıkış</label>
                        </div>
                    </div>
                    <hr />
                    <div class="form-group">
                        <div class="custom-control custom-radio">
                            <input type="radio" value="3" class="custom-control-input" id="tümKayit" checked name="Gecis_Tipi">
                            <label class="custom-control-label" for="tümKayit">Tüm Kayıtlar</label>
                        </div>
                        <div class="custom-control custom-radio">
                            <input type="radio" value="1" class="custom-control-input" id="OnaylananGecisler" name="Gecis_Tipi">
                            <label class="custom-control-label text-success" for="OnaylananGecisler">Onaylanan Geçişler</label>
                        </div>
                        <div class="custom-control custom-radio">
                            <input type="radio" value="2" class="custom-control-input" id="Antipassback" name="Gecis_Tipi">
                            <label class="custom-control-label text-danger" for="Antipassback">Antipassback İhlalleri</label>
                        </div>
                        <div class="custom-control custom-radio">
                            <input type="radio" value="0" class="custom-control-input" id="Engellenen" name="Gecis_Tipi">
                            <label class="custom-control-label text-warning" for="Engellenen">Engellenen İstekler</label>
                        </div>
                    </div>
                    <hr />
                    <div class="form-group">
                        <div class="custom-control custom-checkbox">
                            <input type="checkbox" value="true" name="Tümü" class="custom-control-input" id="TumKapi" />
                            <label class="custom-control-label" for="TumKapi">Tüm Kapılar</label>
                        </div>
                        <div id="KapiListesi"></div>
                    </div>
                </div>
                <div class="col-md-2">
                    <div class="form-group">
                        <div class="custom-control custom-checkbox">
                            <input type="checkbox" value="true" name="Tum_Tarih" class="custom-control-input" id="TümTarih" />
                            <label class="custom-control-label" for="TümTarih">Tüm Tarihler</label>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="custom-control custom-checkbox">
                            <input type="checkbox" value="true" name="ikiTarih" class="custom-control-input" id="ikiTarih" />
                            <label class="custom-control-label" for="ikiTarih">İki Tarih Arası</label>
                        </div>
                        <div class="form-group">
                            <label for="exampleFormControlInput1">Başlangıç</label>
                            <input class="form-control form-control-sm" name="Baslangic_Tarihi" type="date" value="2019-08-13" id="Tarih1">
                        </div>
                        <div class="form-group">
                            <label for="exampleFormControlInput1">Bitiş</label>
                            <input class="form-control form-control-sm" name="Bitis_Tarihi" disabled type="date" value="" id="Tarih2">
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="custom-control custom-checkbox">
                            <input type="checkbox" value="true" name="ikiSaat" class="custom-control-input" id="ikiSaat" />
                            <label class="custom-control-label" for="ikiSaat">İki Saat Arası</label>
                        </div>
                        <div class="form-group">
                            <input class="form-control form-control-sm" name="Baslangic_Saati" type="time" disabled value="@DateTime.Now.ToShortTimeString()" id="Saat1">
                        </div>
                        <div class="form-group">
                            <input class="form-control form-control-sm" name="Bitis_Saati" type="time" disabled value="@DateTime.Now.ToShortTimeString()" id="Saat2">
                        </div>
                        <div class="custom-control custom-checkbox">
                            <input type="checkbox" value="true" name="Gunluk_Saat_Dilimi" class="custom-control-input" id="Günlük" />
                            <label class="custom-control-label" for="Günlük">Günlük Saat Dilimi Bazında</label>
                        </div>
                    </div>
                </div>
                <div class="col-md-2">
                    <div class="form-group">
                        <label>Şirketler</label>
                        @Html.DropDownListFor(model => model.Sirket, Model.Sirket, "Şirket Seçiniz..", new { @class = "form-control form-control-sm chosen-select", tabindex = "2" })
                    </div>
                    <div class="form-group">
                        <label>Departmanlar</label>
                        @Html.DropDownListFor(model => model.Departman, Model.Departman, "Departman Seçiniz..", new { @class = "form-control form-control-sm chosen-select", tabindex = "2" })
                    </div>
                    <div class="form-group">
                        <label>Alt Departmanlar</label>
                        @Html.DropDownListFor(model => model.AltDepartman, Model.AltDepartman, "Alt Departman Seçiniz..", new { @class = "form-control form-control-sm chosen-select", tabindex = "2" })
                    </div>
                    <div class="form-group">
                        <label>Bölümler</label>
                        @Html.DropDownListFor(model => model.Bolum, Model.Bolum, "Bölüm Seçiniz..", new { @class = "form-control form-control-sm chosen-select", tabindex = "2" })
                    </div>
                    @*<div class="form-group">
                            <label>Birimler</label>
                            @Html.DropDownListFor(model => model.Birim_No, Model.Birim_No, "Birim Seçiniz..", new { @class = "form-control form-control-sm chosen-select", tabindex = "2" })
                        </div>*@

                    <div class="form-group">
                        @*<div class="custom-control custom-checkbox">
                                <input type="checkbox" value="true" name="Tum_Kullanici" class="custom-control-input" id="TümKullanici" />
                                <label class="custom-control-label mb-2" for="TümKullanici">Tüm Aktif Kullanıcılar</label>
                            </div>*@
                        <label for="Ziyaretci">Kullanıcılar</label>
                        <div class="form-group">
                            <input type="text" id="Ziyaretci" class="form-control form-control-sm" name="Users" />
                            <input type="text" id="UserKayitNo" hidden name="User" value="" />
                        </div>
                    </div>
                </div>
                <div class="col-md-2">
                    <div class="form-group">
                        <label>Paneller</label>
                        <div class="form-group">
                            @Html.DropDownListFor(model => model.Panel, Model.Panel, "Panel Seçimi", new { @class = "form-control form-control-sm chosen-select", tabindex = "2" })

                        </div>
                    </div>

                    <div class="form-group">
                        <label>Ünvan</label>
                        <div class="form-group">
                            @Html.DropDownListFor(model => model.Unvan, Model.Unvan, "Ünvan Seçimi", new { @class = "form-control form-control-sm chosen-select", tabindex = "2" })
                        </div>
                    </div>
                    <div class="form-group">
                        <label>Geçiş Grupları</label>
                        <div class="form-group">
                            @Html.DropDownListFor(model => model.Gecis_Grubu, Model.Gecis_Grubu, "Geçiş Grubu Seçimi", new { @class = "form-control form-control-sm chosen-select", tabindex = "2" })
                        </div>
                    </div>
                    <div class="form-group">
                        <label>Bloklar</label>
                        @Html.DropDownListFor(model => model.Blok, Model.Blok, "Blok Seçiniz..", new { @class = "form-control form-control-sm chosen-select", tabindex = "2" })

                    </div>
                    <div class="form-group">
                        <label>Global Kapı Bölgesi</label>
                        <div class="form-group">
                            @Html.DropDownListFor(model => model.Global_Kapi_Bolgesi, Model.Global_Kapi_Bolgesi, "Global Kapı Bölgesi Seçimi", new { @class = "form-control form-control-sm chosen-select", tabindex = "2" })
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="Plaka">Plaka Seçimi</label>
                        <input type="text" placeholder="Plaka" class="form-control form-control-sm" id="Plaka" name="Plaka" value="" />
                    </div>

                </div>
                <div class="col-md-4">
                    <img id="change-image" src="@img" style="height:300px; width:300px;" alt="Kayıtlı Fotoğraf" />
                    <hr />
                    <img id="change-image-canli" src="@canlimg" style="height:300px; width:300px;" alt="Canlı Fotoğraf" />
                    <br /><br /><br />
                    <h4 class="text-info text-right">@Model.ReportPersonel.Count Adet Kayıt Listelendi</h4>
                    <hr />
                    <button type="submit" class="btn btn-sm btn-secondary"><i class="fas fa-chart-bar"></i> Rapor Oluştur</button>
                    <a id="Excell" href="@Url.Action("AktifPersonelRaporları", "ReportPersonel")" class="btn  btn-sm btn-success"><i class="fas fa-file-export"></i>Excell'e Aktar</a>
                    <button type="button" id="btnPrint" class="btn  btn-sm btn-info"><i class="fa fa-print"></i> Yazdır</button>


                </div>
            </div>
        }

        <div class="row">
            <div class="col-md-12">
                <div id="print_div">
                    @if (Model.ReportPersonel.Count > 0)
                    {
                        @webGrid.GetHtml(
        htmlAttributes: new { @id = "table", @class = "table-borderless table table-hover text-center table-striped table-sm" },
        selectedRowStyle: "selectRow",
        footerStyle: "table-pager",
        columns: webGrid.Columns(
        webGrid.Column("Kart_ID", "Canlı/Kayıtlı", format: (item) => item.GetSelectLink("Resim")),
        webGrid.Column("ID", "ID"),
        webGrid.Column("Kart_ID", "Kart ID"),
        webGrid.Column("UnvanAdi", "Ünvan"),
        webGrid.Column("Adi", "Adı"),
        webGrid.Column("Soyadi", "Soyadı"),
        webGrid.Column("TCKimlik", "TC Kimlik"),
        webGrid.Column("Telefon", "Telefon"),
        webGrid.Column("SirketAdi", "Şirket"),
        webGrid.Column("DepartmanAdi", "Departman"),
        webGrid.Column("AltDepartmanAdi", "Alt Departman"),
        webGrid.Column("BolumAdi", "Bölüm"),
       // webGrid.Column("BirimAdi", "Birim"),
        webGrid.Column("Grup_Adi", "Grup"),
        webGrid.Column("Panel_ID", "Panel"),
        webGrid.Column("Kapi", "Kapı"),
        webGrid.Column("Gecis_Tipi", "Geçiş", format: item => item.Gecis_Tipi == 0 ? "Giriş" : "Çıkış"),
        webGrid.Column("Tarih", "Tarih")))

                    }
                    else
                    {
                        <div class="alert alert-info alert-message">
                            <h4 class="text-center text-muted">Kriterlere Uygun Kayıt Yok!</h4>
                        </div>

                    }

                </div>
            </div>
        </div>
    </div>
</div>
<!-- Modal Aktif -->
<div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-xl modal-dialog-scrollable" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="text-muted">Kullanıcı Arama</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body ">
                <table id="myTable" class="table-borderless table table-hover text-center table-striped table-sm" style="width:100%;">
                    <thead>
                        <tr>
                            <td>Kayıt No</td>
                            <td>Kart ID</td>
                            <td>Adı</td>
                            <td>Soyadı</td>
                            <td>TC Kimlik</td>
                            <td>Departman</td>
                            <td>Alt Departman</td>
                            <td>Bölüm</td>
                            <td>Geçiş Grubu</td>

                        </tr>
                    </thead>
                    <tbody id="zyrt">
                        @foreach (var item in Model.Kullanıcı)
                        {
                            <tr>
                                <td data-kayitno="@item.Kayit_No" data-visitor="@(item.Adi+" "+item.Soyadi )">@item.Kayit_No</td>
                                <td>@item.Kart_ID</td>
                                <td>@item.Adi</td>
                                <td>@item.Soyadi</td>
                                <td>@item.TCKimlik</td>
                                <td>@item.DepartmanAdi</td>
                                <td>@item.AltDepartmanAdi</td>
                                <td>@item.BolumAdi</td>
                                <td>@item.Grup_Adi</td>
                            </tr>

                        }
                    </tbody>
                </table>
            </div>
            <div class="modal-footer">
                <button type="button" id="temizle" class="btn btn-success"><i class="fas fa-trash-alt"></i> Seçimi İptal Et</button>
                <button type="button" id="close" class="btn btn-secondary " data-dismiss="modal"><i class="fas fa-times"></i> Kapat</button>
            </div>
        </div>
    </div>
</div>

