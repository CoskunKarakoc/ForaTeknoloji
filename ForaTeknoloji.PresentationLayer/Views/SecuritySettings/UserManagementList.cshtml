@model ForaTeknoloji.Entities.Entities.OperatorTransactionList
@{
    ViewBag.Title = "UserManagementList";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="row">
    <div class="col-md-8 offset-2">
        <div class="card shadow-lg mx-lg-5 my-lg-5 animated fadeIn border-left-info h-100 faster">
            <div class="card-header py-1 mt-1">
                <h3 class="card-title"><i class="fas fa-bookmark"></i> Kullanıcı Yönetim Listesi</h3>
            </div>
            <div class="card-body">
                @using (Html.BeginForm("UserManagementList", "SecuritySettings", FormMethod.Post))
                {
                    <div class="form-group row">
                        <label for="Kullanici_Adi" class="col-sm-4 col-form-label text-right">Kullanıcı Adı</label>
                        <div class="col-sm-5">
                            <input type="text" hidden name="Kullanici_Adi_Yonetim_Listesi" value="@Model.Kullanici_Adi_Yonetim_Listesi" />
                            @Html.EditorFor(model => model.Kullanici_Adi_Yonetim_Listesi, null, "Kullanici_Adi_Yonetim_Listesi", new { htmlAttributes = new { @class = "form-control form-control-sm", disabled = true } })
                            @Html.ValidationMessageFor(model => model.Kullanici_Adi_Yonetim_Listesi, "", new { @class = "text-danger" })
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="col-md-6">
                            <div class="custom-control custom-checkbox">
                                @Html.CheckBox("Canli_Izleme", Model.Canli_Izleme ?? false, new { @class = "custom-control-input" })
                                <label class="custom-control-label " for="Canli_Izleme">Canlı İzleme</label>
                            </div>
                            <div class="custom-control custom-checkbox">
                                @Html.CheckBox("Spot_Monitor", Model.Spot_Monitor ?? false, new { @class = "custom-control-input" })
                                <label class="custom-control-label " for="Spot_Monitor">Spot Monitör</label>
                            </div>
                            <div class="custom-control custom-checkbox">
                                @Html.CheckBox("Panel_Ekleme", Model.Panel_Ekleme ?? false, new { @class = "custom-control-input" })
                                <label class="custom-control-label" for="Panel_Ekleme">Panel Ekleme</label>
                            </div>
                            <div class="custom-control custom-checkbox">
                                @Html.CheckBox("Panel_Guncelleme", Model.Panel_Guncelleme ?? false, new { @class = "custom-control-input" })
                                <label class="custom-control-label " for="Panel_Guncelleme">Panel Güncelleme</label>
                            </div>
                            <div class="custom-control custom-checkbox">
                                @Html.CheckBox("Panel_Ayar_Gonderme", Model.Panel_Ayar_Gonderme ?? false, new { @class = "custom-control-input" })
                                <label class="custom-control-label" for="Panel_Ayar_Gonderme">Panel Ayar Gönderme</label>
                            </div>
                            <div class="custom-control custom-checkbox">
                                @Html.CheckBox("Zaman_Bolgesi_Ayarlari", Model.Zaman_Bolgesi_Ayarlari ?? false, new { @class = "custom-control-input" })
                                <label class="custom-control-label " for="Zaman_Bolgesi_Ayarlari">Zaman_Bölgesi Ayarları</label>
                            </div>
                            <div class="custom-control custom-checkbox">
                                @Html.CheckBox("Gecis_Grup_Ayarlari", Model.Gecis_Grup_Ayarlari ?? false, new { @class = "custom-control-input" })
                                <label class="custom-control-label" for="Gecis_Grup_Ayarlari">Geciş Grup Ayarları</label>
                            </div>
                            <div class="custom-control custom-checkbox">
                                @Html.CheckBox("Kullanici_Ekleme", Model.Kullanici_Ekleme ?? false, new { @class = "custom-control-input" })
                                <label class="custom-control-label " for="Kullanici_Ekleme">Kullanıcı Ekleme</label>
                            </div>
                            <div class="custom-control custom-checkbox">
                                @Html.CheckBox("Kullanici_Duzenleme", Model.Kullanici_Duzenleme ?? false, new { @class = "custom-control-input" })
                                <label class="custom-control-label" for="Kullanici_Duzenleme">Kullanıcı Düzenleme</label>
                            </div>
                            <div class="custom-control custom-checkbox">
                                @Html.CheckBox("Kullanici_Gonderme", Model.Kullanici_Gonderme ?? false, new { @class = "custom-control-input" })
                                <label class="custom-control-label" for="Kullanici_Gonderme">Kullanıcı Gönderme</label>
                            </div>
                            <div class="custom-control custom-checkbox">
                                @Html.CheckBox("Ziyaretci_Ekleme", Model.Ziyaretci_Ekleme ?? false, new { @class = "custom-control-input" })
                                <label class="custom-control-label" for="Ziyaretci_Ekleme">Ziyaretçi Ekleme</label>
                            </div>
                            <div class="custom-control custom-checkbox">
                                @Html.CheckBox("Ziyaretci_Duzenleme", Model.Ziyaretci_Duzenleme ?? false, new { @class = "custom-control-input" })
                                <label class="custom-control-label" for="Ziyaretci_Duzenleme">Ziyaretçi Düzenleme</label>
                            </div>
                            <div class="custom-control custom-checkbox">
                                @Html.CheckBox("Ziyaretci_Gonderme", Model.Ziyaretci_Gonderme ?? false, new { @class = "custom-control-input" })
                                <label class="custom-control-label" for="Ziyaretci_Gonderme">Ziyaretçi Gönderme</label>
                            </div>
                            <div class="custom-control custom-checkbox">
                                @Html.CheckBox("Kapi_Operasyon", Model.Kapi_Operasyon ?? false, new { @class = "custom-control-input" })
                                <label class="custom-control-label" for="Kapi_Operasyon">Kapı Operasyon</label>
                            </div>
                            <div class="custom-control custom-checkbox">
                                @Html.CheckBox("Kapi_Grup_Olusturma", Model.Kapi_Grup_Olusturma ?? false, new { @class = "custom-control-input" })
                                <label class="custom-control-label" for="Kapi_Grup_Olusturma">Kapı Grup Oluşturma</label>
                            </div>
                            <div class="custom-control custom-checkbox">
                                @Html.CheckBox("Grup_Takvimi_Olusturma", Model.Grup_Takvimi_Olusturma ?? false, new { @class = "custom-control-input" })
                                <label class="custom-control-label" for="Grup_Takvimi_Olusturma">Grup Takvimi Oluşturma</label>
                            </div>
                            <div class="custom-control custom-checkbox">
