                                @Html.CheckBox("Gecis_Olay_Verileri", Model.Gecis_Olay_Verileri ?? false, new { @class = "custom-control-input" })
                                <label class="custom-control-label" for="Gecis_Olay_Verileri">Geciş Olay Verileri</label>
                            </div>
                            <div class="custom-control custom-checkbox">
                                @Html.CheckBox("Gecis_Olay_Yedekle", Model.Gecis_Olay_Yedekle ?? false, new { @class = "custom-control-input" })
                                <label class="custom-control-label" for="Gecis_Olay_Yedekle">Geciş Olay Yedekle</label>
                            </div>
                            <div class="custom-control custom-checkbox">
                                @Html.CheckBox("Personel_Listesi", Model.Personel_Listesi ?? false, new { @class = "custom-control-input" })
                                <label class="custom-control-label" for="Personel_Listesi">Personel Listesi</label>
                            </div>
                            <div class="custom-control custom-checkbox">
                                @Html.CheckBox("Aktif_Personel_Raporlari", Model.Aktif_Personel_Raporlari ?? false, new { @class = "custom-control-input" })
                                <label class="custom-control-label" for="Aktif_Personel_Raporlari">Aktif Personel Raporları</label>
                            </div>
                            <div class="custom-control custom-checkbox">
                                @Html.CheckBox("Eski_Personel_Raporlari", Model.Eski_Personel_Raporlari ?? false, new { @class = "custom-control-input" })
                                <label class="custom-control-label" for="Eski_Personel_Raporlari">Eski Personel Raporları</label>
                            </div>
                            <div class="custom-control custom-checkbox">
