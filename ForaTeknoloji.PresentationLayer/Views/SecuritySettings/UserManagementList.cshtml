                                <label class="custom-control-label" for="Gelen_Kisi_Raporlari">Gelen Kişi Raporları</label>
                            </div>
                            <div class="custom-control custom-checkbox">
                                @Html.CheckBox("Gelmeyen_Kisi_Raporlari", Model.Gelmeyen_Kisi_Raporlari ?? false, new { @class = "custom-control-input" })
                                <label class="custom-control-label" for="Gelmeyen_Kisi_Raporlari">Gelmeyen Kişi Raporları</label>
                            </div>
                            <div class="custom-control custom-checkbox">
                                @Html.CheckBox("Pasif_Kullanici_Raporlari", Model.Pasif_Kullanici_Raporlari ?? false, new { @class = "custom-control-input" })
                                <label class="custom-control-label" for="Pasif_Kullanici_Raporlari">Pasif Kullanıcı Raporları</label>
                            </div>
                            <div class="custom-control custom-checkbox">
                                @Html.CheckBox("Toplu_Giris_Sayisi_Raporlari", Model.Toplu_Giris_Sayisi_Raporlari ?? false, new { @class = "custom-control-input" })
                                <label class="custom-control-label" for="Toplu_Giris_Sayisi_Raporlari">Toplu Giriş Sayısı Raporları</label>
                            </div>
                            <div class="custom-control custom-checkbox">
                                @Html.CheckBox("Icerde_Disarda_Personel_Raporlari", Model.Icerde_Disarda_Personel_Raporlari ?? false, new { @class = "custom-control-input" })
                                <label class="custom-control-label" for="Icerde_Disarda_Personel_Raporlari">İçerde Dişarda Personel Raporları</label>
                            </div>
                            <div class="custom-control custom-checkbox">
                                @Html.CheckBox("Icerde_Disarda_Ziyaretci_Raporlari", Model.Icerde_Disarda_Ziyaretci_Raporlari ?? false, new { @class = "custom-control-input" })
                                <label class="custom-control-label" for="Icerde_Disarda_Ziyaretci_Raporlari">İçerde Dışarda Ziyaretçi Raporları</label>
                            </div>
                            <div class="custom-control custom-checkbox">
                                @Html.CheckBox("Icerde_Disarda_Tumu", Model.Icerde_Disarda_Tumu ?? false, new { @class = "custom-control-input" })
                                <label class="custom-control-label" for="Icerde_Disarda_Tumu">İçerde Dışarda Tümü</label>
                            </div>
                            <div class="custom-control custom-checkbox">
                                @Html.CheckBox("Yemekhane_Kisi_Bazli_Rapor", Model.Yemekhane_Kisi_Bazli_Rapor ?? false, new { @class = "custom-control-input" })
                                <label class="custom-control-label" for="Yemekhane_Kisi_Bazli_Rapor">Yemekhane Kişi Bazlı Rapor</label>
                            </div>
                            <div class="custom-control custom-checkbox">
                                @Html.CheckBox("Yemekhane_Toplu_Gecis_Sayisi_Raporlari", Model.Yemekhane_Toplu_Gecis_Sayisi_Raporlari ?? false, new { @class = "custom-control-input" })
                                <label class="custom-control-label" for="Yemekhane_Toplu_Gecis_Sayisi_Raporlari">Yemekhane Toplu Geçiş Sayısı Raporları</label>
                            </div>
