                                <label class="custom-control-label" for="Aktif_Olmayanlar_Listesi">Aktif Olmayanlar Listesi</label>
                            </div>
                            <div class="custom-control custom-checkbox">
                                @Html.CheckBox("Ilk_Giris_Son_Cikis_Raporlari", Model.Ilk_Giris_Son_Cikis_Raporlari ?? false, new { @class = "custom-control-input" })
                                <label class="custom-control-label" for="Ilk_Giris_Son_Cikis_Raporlari">İlk Giriş Son Cıkış Raporları</label>
                            </div>
                            <div class="custom-control custom-checkbox">
