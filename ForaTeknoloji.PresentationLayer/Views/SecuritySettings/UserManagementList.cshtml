                                @Html.CheckBox("Ziyaretci_Raporlari", Model.Ziyaretci_Raporlari ?? false, new { @class = "custom-control-input" })
                                <label class="custom-control-label" for="Ziyaretci_Raporlari">Ziyaretçi Raporları</label>
                            </div>
