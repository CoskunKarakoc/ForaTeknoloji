@model ForaTeknoloji.Entities.Entities.TimeGroups
@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-md-8 offset-md-2">
        <div class="card shadow-lg mt-xl-5 mb-xl-5">
            <div class="card-header-tabs mt-1">
                <h3 class="text-muted text-center">Zaman Bölgesi Ayarları</h3>
            </div>
            <div class="card-body">
                @using (Html.BeginForm())
                {
                    @Html.AntiForgeryToken()
                    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                    @Html.HiddenFor(model => model.Zaman_Grup_No)

                    <hr />
                    <div class="row">
                        <div class="col-md-4">
                            <div class="form-group">
                                <label class="control-label col-md-12">Zaman Bölge No</label>
                                <div class="col-md-12">
                                    <div class="form-group">
                                        @Html.EditorFor(model => model.Zaman_Grup_No, new { htmlAttributes = new { @class = "form-control", disabled = "true" } })
                                        @Html.ValidationMessageFor(model => model.Zaman_Grup_No, "", new { @class = "text-danger" })
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="control-label col-md-12">Zaman Bölge Adı</label>
                                <div class="col-md-12">
                                    <div class="form-group">
                                        @Html.EditorFor(model => model.Zaman_Grup_Adi, new { htmlAttributes = new { @class = "form-control" } })
                                        @Html.ValidationMessageFor(model => model.Zaman_Grup_Adi, "", new { @class = "text-danger" })
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="control-label col-md-12">Zaman Bölge Tipi</label>
                                <div class="col-md-12">
                                    <div class="form-group">
                                        @Html.DropDownList("Gecis_Sinirlama_Tipi", null, "", new { @class = "form-control" })
                                        @Html.ValidationMessageFor(model => model.Gecis_Sinirlama_Tipi, "", new { @class = "text-danger" })
                                    </div>
                                </div>
                            </div>

                            <div class="col-md-12 text-right">
                                <button type="submit" class="btn btn-sm btn-outline-success"><i class="fa fa-edit"></i> Güncelle</button>
                                <a href="@Url.Action("Index","TimeGroups")" class="btn btn-sm btn-outline-info"><i class="fa fa-hand-point-left"></i> Listeye Geri Dön</a>
                            </div>

                        </div>
                        <div class="col-md-8">
                            <div hidden id="Tarih">
                                <div class="form-group">
                                    <label class="control-label col-md-12"> Başlangıç Tarihi</label>
                                    @if (Model.Bitis_Tarihi == null)
                                    {
                                        <input type="date" class="form-control" name="Baslangic_Tarihi" id="Baslangic_Tarihi" />
                                    }
                                    else
                                    {
                                        @Html.TextBoxFor(model => model.Baslangic_Tarihi, "{0:yyyy-MM-dd}", new { @class = "form-control", @type = "date" })
                                    }
                                </div>
                                <div class="form-group">
                                    <label class="control-label col-md-12">Bitiş Tarihi</label>
                                    @if (Model.Bitis_Tarihi == null)
                                    {
                                        <input type="date" class="form-control" name="Bitis_Tarihi" id="Bitis_Tarihi" />
                                    }
                                    else
                                    {
                                        @Html.TextBoxFor(model => model.Bitis_Tarihi, "{0:yyyy-MM-dd}", new { @class = "form-control", @type = "date" })
                                    }
                                </div>
                            </div>
